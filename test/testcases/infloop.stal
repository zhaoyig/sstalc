_entry:
    code[$p]{ sp: {sp : $p} :: $p }.
        mov rax, _entry[$p]
        salloc 1
        sst sp(0), rax
        jmp _body[{sp : $p} :: $p]
_body:
    code[$p]{ sp: {sp : $p} :: $p }.
        sld rdx, sp(0)
        sfree 1
        jmp rdx
_halt:
    code[]{ sp: nil }.
        mov rdi, $0
        halt int
_main:
    code[]{ sp: nil }.
        salloc 1
        mov rax, _halt
        sst sp(0), rax
        jmp _entry[nil]